            <script>
                const userIcon = document.querySelector('.user-icon')
                const accountOptions = document.querySelector('.user-account-options')

                // declare this variable outside the showOptions fn, so the event listener that will be assigned to it can also be removed from outside showOptions
                let outsideClickListener

                // display the user account options
                const showOptions = () => {
                    accountOptions.classList.remove('hidden')

                    // hide user account options & rm event listener if when clicking outside the userIcon OR outside the account options element itself
                    outsideClickListener = (e) => {
                        if (!accountOptions.contains(e.target) && !userIcon.contains(e.target)) {
                            hideOptions()
                            document.removeEventListener('pointerdown', outsideClickListener)
                        }
                    }
                    
                    // add ev listener using setTimeout, so we don't add it while the click that made it disappear is still bubbling
                    setTimeout(() => {
                        document.addEventListener('pointerdown', outsideClickListener)
                    }, 0)
                }

                const hideOptions = () => {
                    accountOptions.classList.add('hidden')
                    document.removeEventListener('pointerdown', outsideClickListener)
                }

                userIcon.addEventListener('pointerdown', () => {
                    showOptions()
                })
            </script>
